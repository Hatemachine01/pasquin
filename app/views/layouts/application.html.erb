<!DOCTYPE html>
<html>
  <head>
    <title> Pasquin </title>
  
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
 
 <!-- Google Fonts -->
  <link href='https://fonts.googleapis.com/css?family=Open+Sans:400,600,700' rel='stylesheet'>

  <!-- Css -->
  <link rel="stylesheet" href="/assets/bootstrap.min.css" />
  <link rel="stylesheet" href="/assets/font-icons.css" />
  <link rel="stylesheet" href="/assets/style.css" />




  
  </head>
  	<p class="notice"><%= notice %></p>
	<p class="alert"><%= alert %></p>
  
    
<body class="bg-light single-post">


  <!-- Preloader -->
  <div class="loader-mask">
    <div class="loader">
      <div></div>
    </div>
  </div>


  <!-- Nav Bar -->

  <%= render 'shared/nav' %>
  
  <!-- Renders Current Controller View -->

    <%= yield %>

  <!-- Footer -->
   
  <%= render 'shared/footer' %> 

    <!-- jQuery Scripts -->
    <script src="/assets/jquery.min.js"></script>
    <script src="/assets/bootstrap.min.js"></script>
    <script src="/assets/easing.min.js"></script>
    <script src="/assets/owl-carousel.min.js"></script>
    <script src="/assets/twitterFetcher_min.js"></script>
    <script src="/assets/jquery.newsTicker.min.js"></script>  
    <script src="/assets/modernizr.min.js"></script>
    <script src="/assets/scripts.js"></script>
  </body>
</html>


